def containsNearbyDuplicate(nums, k):
    matrix = []
    for i in range(len(nums)):
        matrix.append([nums[i], i])
    matrix = sorted(matrix, key=lambda x: x[0])
    for i in range(1, len(matrix)):
        if matrix[i][0] == matrix[i - 1][0] and abs(matrix[i][1] - matrix[i - 1][1]) <= k:
            return True
    return False

print(containsNearbyDuplicate(nums = [1,2,3,1], k = 3))
